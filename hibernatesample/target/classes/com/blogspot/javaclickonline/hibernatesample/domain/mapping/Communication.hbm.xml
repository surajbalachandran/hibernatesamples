<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.blogspot.javaclickonline.hibernatesample.domain">
	<class name="Communication" table="JCO_COMMUNICATION">
		<id name="communicationId" column="COMMUNICATION_ID">
			<generator class="native"></generator>
		</id>
		<property name="businessPhoneNo" column="BUS_PHONE" />
		<property name="homePhoneNo" column="HOME_PHONE" />
		<property name="faxNo" column="FAX_PHONE" />
	</class>
</hibernate-mapping>